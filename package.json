{
  "name": "test-cra",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jest-junit": "^5.2.0",
    "react-scripts": "2.0.4"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "jest-junit": {
    "outputDirectory": "test-results",
    "outputName": "jest/results.xml"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "test:ci": "react-scripts test --coverage --reporters=default --reporters=./node_modules/jest-junit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
